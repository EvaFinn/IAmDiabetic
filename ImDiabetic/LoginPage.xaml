<?xml version="1.0" encoding="UTF-8"?>
<ContentPage Title="Login" NavigationPage.HasBackButton="False" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="ImDiabetic.LoginPage">
    <StackLayout HorizontalOptions="Center" VerticalOptions="Center">
        <Label Text="Login" HorizontalOptions="Center"/>
        <Entry x:Name="firstNameEntry" Placeholder="First Name" />
        <Entry x:Name="passwordEntry" Placeholder="Password" IsPassword="true" />
        <Button Text="Next" Clicked="Handle_Clicked"/>
        <Button Text="Register" Clicked="Register_Clicked"/>
    </StackLayout>
</ContentPage>
